{
  "expo": {
    "name": "Polytalk",
    "slug": "polytalk",
    "version": "1.0.0",
    "scheme": "polytalk",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/logo-b.png",
          "dark": {
            "image": "./assets/logo-w.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-router",
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true,
      "reactCompiler": true
    },
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.polytalk.app",
      "appleTeamId": "PHSD73G5UF",
			"usesAppleSignIn": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-b.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.polytalk.app",
      "edgeToEdgeEnabled": true
    }
  }
}
